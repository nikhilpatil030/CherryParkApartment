{
  "name": "cherrypark-api",
  "version": "1.0.0",
  "description": "CherryPark Apartment Management API",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "npx tsc && node dist/index.js",
    "build": "npx tsc",
    "backend": "npx tsc && node dist/index.js",
    "frontend": "cd ../frontend && ng serve --o",
    "cherryPark": "npm-run-all --parallel backend frontend",
    "lint": "eslint 'src/**/*.ts'",
    "lint:fix": "eslint 'src/**/*.ts' --fix",
    "format": "prettier --write 'src/**/*.ts'",
    "format:check": "prettier --check 'src/**/*.ts'",
    "test": "echo 'Tests will be added' && exit 0",
    "test:watch": "echo 'Tests will be added' && exit 0",
    "test:coverage": "echo 'Tests will be added' && exit 0"
  },
  "keywords": ["apartment-management", "api", "express", "mongodb"],
  "author": "CherryPark Team",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^4.21.0",
    "express-rate-limit": "^8.2.1",
    "express-validator": "^7.3.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^4.1.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "winston": "^3.14.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.12",
    "@types/node": "^20.11.0",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.2.4",
    "typescript": "^5.3.3"
  }
}
